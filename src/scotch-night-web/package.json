    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.0",
    "webpack-dev-server": "^2.9.2",
    "webpack-hot-middleware": "^2.20.0",
    "yarn": "^1.2.1"
  },
  "dependencies": {
    "auth0-js": "^8.10.1",
    "date-fns": "^1.29.0",
    "fs": "^0.0.1-security",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
