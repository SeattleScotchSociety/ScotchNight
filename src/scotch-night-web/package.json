    "path": "^0.12.7",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.1.3",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
