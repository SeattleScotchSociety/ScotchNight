{
    "name": "scotch-night-web",
    "version": "0.0.2",
    "main": "index.ts",
    "repository": "https://github.com/SeattleScotchSociety/ScotchNight",
    "author": "Seattle Scotch Society",
    "license": "MIT",
    "scripts": {
        "prebuild:dev": "rimraf dist",
        "build:dev": "webpack --env.dev",
        "prebuild": "rimraf dist",
        "build": "webpack -p --env.prod",
        "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
        "start": "webpack-dev-server --env.prod",
        "start:dashboard": "webpack-dashboard -- webpack-dev-server --env.dev",
        "test": "jest --no-cache"
    },
    "devDependencies": {
        "@types/auth0-js": "8.11.2",
        "@types/googlemaps": "3.30.8",
        "@types/history": "4.6.2",
